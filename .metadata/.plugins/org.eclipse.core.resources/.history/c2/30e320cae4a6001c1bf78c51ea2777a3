class A
{
	void sub(int a,int b) {
		System.out.println("=====>"+(a-b));
		
	}
}
class B extends A{
	void sub(int a,int b) {
		System.out.println("=====>"+(a+(b*a)));
		
	}
}
public class InheritanceEx {
	

	public static void main(String[] args) {
		// TODO Auto-generated method stub
A ob=new B();
ob.sub(3, 4);
		
	}

}
